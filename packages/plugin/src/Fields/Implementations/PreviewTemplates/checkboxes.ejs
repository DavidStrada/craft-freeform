<% generatedOptions.forEach(function(option) { %>

    <% if (oneLine) { %><span><% } else { %><div><% } %>

        <input type="checkbox"
               class="checkbox"
               <% if (defaultValue.includes(option.value)) { %>checked="checked"<% } %>
        />
        <label for=""><%= option.label %></label>

    <% if (oneLine) { %><span><% } else { %></div><% } %>

<% }) %>
